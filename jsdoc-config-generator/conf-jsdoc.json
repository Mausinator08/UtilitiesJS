{
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "recurse": true,
        "destination": ".\\docs\\"
    },
    "recurseDepth": 10,
    "source": {
        "include": [],
        "includePattern": ".+\\.{{type}}(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}